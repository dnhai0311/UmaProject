{
  "name": "uma-project-app",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.0",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.2.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "scrape": "node scrape/skill-scrape.js && node scrape/support-scrape.js && node scrape/uma-scrape.js",
    "copy-data": "node -e \"const fs = require('fs'); const path = require('path'); if (fs.existsSync('scrape/data')) { fs.cpSync('scrape/data', 'public/data', { recursive: true }); console.log('Data copied successfully'); } else { console.log('No data to copy'); }\"",
    "scrape-all": "npm run scrape && npm run copy-data"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
